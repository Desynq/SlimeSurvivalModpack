{
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "../../kubejs/server_scripts/",
    "noEmitOnError": true,
    "allowJs": true,

    "target": "ES5",
    "module": "commonjs",
    "lib": ["ES2017"],

    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": [
    "./**/*.ts",
    "./**/*.js",

    // Pull in only the useful ProbeJS declarations
    "../../.probe/packages/generated-package/net.minecraft.world.entity.d.ts",
    "../../.probe/server/generated-server/events.d.ts"
  ],
  "exclude": [
  ]
}
